{
  "manifest_version": 3,

  "name": "Vaccine Notifier",
  "description": "This extension will notify you when ever Covid vaccine is available in a area",
  "version": "1.3",
  "icons": {
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon128.png",
    "default_popup": "popup.html"
  },
  "background":{
    "scripts": ["background.js"]
  },
  "permissions": [
    "storage",
    "alarms",
    "notifications"
  ]
}
